1) // program to print area and rectangle of circle.
#include<stdio.h>
void main()
{
float l,b,area,perimeter;
printf("Enter the length and breadth of rectangle\n");
scanf("%f%f",&l,&b);
area= l*b;
perimeter= 2*(l+b);
printf("The area of rectangle having lenght:%.2fcm\n breadth:%.2fcm\n is %.2fcm\u00B2\n And perimeter is:%.2fcm",l,b,area,perimeter);
}


2) // program to calculate simple interest and amount 
#include<stdio.h>
void main()
{
int p,t,r,si,amt;
printf("enter principal,time and rate\n");
scanf("%d%d%d",&p,&t,&r);
si=(p*t*r)/100;
amt=p+si;
printf("%d is the Simple interest of given\n principal:%d\n time:%d\n and rate:%d\n and Amount is %d",si,p,t,r,amt);
}

3) // program to print the volume of room and its surface area
#include<stdio.h>
void main()
{
int l,w,h,vol,sa;
printf("enter length, width and height of the room\n");
scanf("%d%d%d",&l,&w,&h);
vol=l*w*h;
sa=2*(l*h+w*h)+2*(l*w);
printf("%d is the volume\n %d is the surface area of room",vol,sa);
}

4) // program to print the area of 4 walls and cost of painting at rs 270 per meter square
#include<stdio.h>
#define rate 270
void main()
{
float l,b,h,area,cost;
printf("enter length, breadth and height of 4 walls of  room\n");
scanf("%f%f%f",&l,&b,&h);
area=2*h*(l+b);
cost=area*rate;
printf("%.2fm\u00B2 is the area of 4 walls of room\n Rs.%.2f is the cost of painting",area,cost);
}

5) // program to print the area and perimeter of circle having any radius 
#include<stdio.h>
#define PI 3.14
void main()
{
int r;
float area,perimeter;
printf("Enter the radius of circle\n");
scanf("%d",&r);
area= 2*PI*r;
perimeter= PI*r*r;
printf("Area of given circle having radius %d cm is %.2f\n And perimeter is %.2f",r,area,perimeter);
}

6) // program to print sum,difference,product and average of any two number
#include<stdio.h>
void main()
{
int a,b,sum,dif,prod,avg;
printf("Enter any two numbers\n");
scanf("%d%d",&a,&b);
sum=a+b;
dif=a-b;
prod=a*b;
avg=sum/2;
printf("Sum is %d\n Difference is %d\n Product is %d\n Average is %d\n Of the number %d and %d",sum,dif,prod,avg,a,b);
}

7) // program to print cube of given number 
#include<stdio.h>
void main()
{
int l,cube;
printf("Enter any number\n");
scanf("%d",&l);
cube=l*l*l;
printf("%d is the cube of number %d",cube,l);
}

8) // program to find out the square root of any number 
#include<stdio.h>
#include<math.h>
void main()
{
float num;
printf("Enter any number to find out square root\n");
scanf("%f",&num);
if (num>=0)
{
printf("The square root of number is %f",sqrt(num));
}
else
{
printf("The number is negative\n Enter a valid positive number\n");
}
}

9) // Program to print sum of cubes of two number
#include<stdio.h>
void main()
{
    float l,b,sum;
    printf("Enter any two number\n");
    scanf("%f%f",&l,&b);
    sum = (l*l*l)+(b*b*b);
    printf("The sum of two given cubes %.2f and %.2f is %.2f",l,b,sum);
}

10) // Program to print normal, integer and modular division of any  two number
#include<stdio.h>
void main ()
{
    int a,b,in,mod;
    float no;
    printf("Enter any two number\n");
    scanf("%d%d",&a,&b);
    if (b==0)
    {
        printf("Oops! You can't divide any numbers by zero idiot!\n\n");
    }
    no= a/b;  
    in= a/b;
    mod=  a % b;
    printf("The normal division of given two number %d by %d  is %.2f\n\n",a,b,no);
    printf("The Integer division of given two number %d by %d is %d\n\n",a,b,in);
    printf("The modular division of given two number %d and %.d is %d",a,b,mod);
}

11) // Program to convert celsius into fahrenheit 
#include<stdio.h>
void main ()
{
    float ce,f;
     printf("Enter Celsius to convert it into Farhenheit\n");
     scanf("%f",&ce);
     f=  ce*9/5+32;
     printf("As per my calculation, the conversion of %.0f%cC is %.1fF”,ce,f);
}

12) //Program to convert Fahrenheit into Celsius 
#include<stdio.h>
void main ()
{
    float f,c;
     printf("Enter Fahrenheit to convert it into Celsius\n");
     scanf("%f",&f);
     c= (f-32)*5.0/9.0;
     printf("As per my calculation, the conversion of %.2f%cF is %.2f%CC”,f,c);
}

13) // Program to convert km into meter 
#include<stdio.h>
void main()
{
    float km,meter;
    printf("Enter the distance(in km) to convert intro meter\n");
    scanf("%f",&km);
    meter= km*1000;
    printf("The conversion of %.2fkm is %.2fm",km,meter);
}

14) // Program to convert nepali currency into usd
#include<stdio.h>
void main()
{
    float nr,usd;
    printf("Enter nepali rupees\n");
    scanf("%f",&nr);
    usd= nr*0.0070;
    printf("The value of Rs.%.0f = %.2fusd",nr,usd);
}

