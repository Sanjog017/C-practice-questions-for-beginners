18) // program to convert inches into feet and inches 
#include<stdio.h>
void main ()
{
    int i,f,e;
    printf("Enter Inches for conversion\n");
    scanf("%d",&i);
      f=i/12;
      e=i%12;
    printf("The conversion of %.dinch is %dfeet and %dinches",i,f,e);
}

19) // program to convert days into years,months,weeks and days 
#include<stdio.h>
void main ()
{
    int d,y,yr,m,mr,w,b;
    printf("Enter days for conversion\n");
    scanf("%d",&d);
     y=d/365;
     yr=d%365;
     m=yr/30;
     mr=yr%30;
     w=mr/7;
     b=mr%7;
    printf("The conversion of %ddays is %d years,%d months,%d weeks and %d days",d,y,m,w,b);
}

20) // program to add last digit and second last digit of a given number 
#include<stdio.h>
void main ()
{
    int n,l,s,sum;
    printf("Enter any number\n");
    scanf("%d",&n);
      l=n%10;
      s=(n/10)%10;
      sum=l+s;
    printf("The last and second last digit of number %d is %d and %d and it's sum is %d",n,l,s,sum);
}

21) // program to calculate distance (s=ut+1/2at2)
#include<stdio.h>
void main()
{
    int u,t,a,s;
    printf("Enter the initial velocity, acceleration and time\n");
    scanf("%d%d%d",&u,&a,&t);
    s=u*t+1/2*a*t*2;
    printf("The distance is %dkm per hour",s);
}

22) // programto find profit from given sp and cp 
#include<stdio.h>
void main () 
{
    int sp,cp;
    printf("Enter sp and cp\n");
    scanf("%d%d",&sp,&cp);
    if (sp>cp)
    {
        printf("There is profit\n");
    }
    else if (cp>sp)
    {
        printf("There is loss\n");
    }
    else 
    {
        printf("There is no profit nor loss\n");
    }
}

23) // program to input perpendicular and base of right angled triangle and compute hypotenuse
#include<stdio.h>
#include<math.h>
void main ()
{
    float p,b,h;
    printf("Enter perpendicular and base of right angled traingle\n");
    scanf("%f%f",&p,&b);
    h= pow(p,2)+pow(b,2);
    printf("The hypotenuse is %.2f",sqrt(h));
}

24) // program to find final velocity 
#include<stdio.h>
void main()
{
    int u,a,t,v;
    printf("Enter initial velocity,acceleration and time\n");
    scanf("%d%d%d",&u,&a,&t);
    v=u+a*t;
    printf("The velocity is %d ",v);
}

25) //program to find area of triangle given its three sides 
#include<stdio.h>
#include<math.h>
void main()
{
    float a,b,c,s,area;
    printf("Enter three sides of trangle\n");
    scanf("%f%f%f",&a,&b,&c);
    s= (a+b+c)/2; //finding semi perimeter 
    area= s*(s-a)*(s-b)*(s-c);
       printf("The area of circle having sides %.2f cm,%.2fcm and %.2fcm is                                            %.2fcm",a,b,c,sqrt(area));
}

26) //program to solve quadratic equation given a,b and c
#include <stdio.h>
#include <math.h>
void main() 
{
    float a, b, c, x, y, w;
    printf("Enter the values of a, b and c\n");
    scanf("%f%f%f",&a,&b,&c);
    w =b*b-4*a*c;
    if (w<0) {
        printf("The roots are imaginary.\n");
    } 
    else 
    {
        x = (-b+sqrt(w))/(2 * a);
        y = (-b-sqrt(w))/(2 * a);
      printf("The roots of the quadratic equation are %.2f and %.2f\n",x,y);
    }
}

27) // program to test whether the given number is completely divisible by 13 and 14 but not by 15 or not.
#include <stdio.h>
void main()
{
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);

    if (n % 13 == 0 && n % 14 == 0 && n % 15 != 0)
    {
        printf("%d is divisible by both 13 and 14, but not by 15.\n", n);
    } 
    else
    {
        printf("%d does not satisfy the condition.\n", n);
    }
}

28) // program to test whether the given number is divisible by 3 and 7 or not 
#include<stdio.h>
void main ()
{
    int n;
    printf("Enter any number\n\t");
    scanf("%d",&n);
    if(n%3==0 && n%7==0)
    {
        printf("The %d is divisible by 3 and 7",n);
    }
    else
    {
        printf("The %d is not divisible by 3 and 7",n);
    }
}

29) // program to test whether the given number is multiple of 5 or not 
#include<stdio.h>
void main ()
{
    int n;
    printf("Enter any number\n");
    scanf("%d",&n);
    if(n%5==0)
    {
        printf("The %d is multiple of 5",n);
    }
    else
    {
        printf("The %d is not multiple of 5",n);
    }
}

